<mvc:View controllerName="ui5TileTrial.controller.Stock" 
xmlns="sap.m"
xmlns:mvc="sap.ui.core.mvc">

	<Table id="idStockHistory" items="{path:'/'}">
		<columns>
			<Column width="12em">
				<Text text="Stock"></Text>
			</Column>
			<Column width="12em">
				<Text text="Date"></Text>
			</Column>
			<Column width="12em">
				<Text text="Pirce"></Text>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{name}"></Text>
					<Text text="{date}"></Text>
					<ObjectNumber number="{price}">
					</ObjectNumber>
				</cells>
			</ColumnListItem>
		</items>
	</Table> 
</mvc:View>